  <ItemGroup>
    <ProjectReference Include="..\..\extensions\Nexus.Extension.Csv\Nexus.Extension.Csv.csproj" />
    <ProjectReference Include="..\..\extensions\Nexus.Extension.Famos\Nexus.Extension.Famos.csproj" />
    <ProjectReference Include="..\..\extensions\Nexus.Extension.Mat73\Nexus.Extension.Mat73.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\RandomWords.json" />
    <Resource Include="Templates\DefaultFilterCodeTemplate.cs" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\EnumerationDescription.cs">
      <DependentUpon>EnumerationDescription.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Resources\EnumerationDescription.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>EnumerationDescription.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\EnumerationIconName.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>EnumerationIconName.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\EnumerationIconName.resx">
      <CustomToolNamespace>Nexus</CustomToolNamespace>
      <LastGenOutput>EnumerationIconName.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\EnumerationDescription.resx">
      <CustomToolNamespace>Nexus</CustomToolNamespace>
      <LastGenOutput>EnumerationDescription.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Connectors\NexusConnector.m">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Connectors\NexusConnector.py">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="OmniSharp\OmniSharpQuickInfoProvider.tt">
      <LastGenOutput>OmniSharpQuickInfoProvider.cs</LastGenOutput>
      <Generator>TextTemplatingFileGenerator</Generator>
    </None>
    <None Update="OmniSharp\OmniSharpSignatureHelpService.tt">
