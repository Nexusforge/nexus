    <Compile Update="OmniSharp\OmniSharpQuickInfoProvider.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>OmniSharpQuickInfoProvider.tt</DependentUpon>
    </Compile>
  </ItemGroup>

  <Import Project="./Roslyn/Generated.targets" />

