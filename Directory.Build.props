<Project>

  <Import Project="./build/build.props" />

  <PropertyGroup>
    <PackageIconUrl>https://raw.githubusercontent.com/Apollo3zehn/Nexus/master/doc/images/icon.png</PackageIconUrl>
    <PackageLicenseExpression Condition="$(PackageLicenseExpression) == ''">LGPL-3.0-only</PackageLicenseExpression>
    <PackageProjectUrl Condition="$(PackageProjectUrl) == ''">https://apollo3zehn.github.io/Nexus/</PackageProjectUrl>
    <RepositoryUrl Condition="$(RepositoryUrl) == ''">https://github.com/Apollo3zehn/Nexus</RepositoryUrl>
  </PropertyGroup>

</Project>